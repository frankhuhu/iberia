<h1>Listing inventories</h1>

<table>
    <tr>
        <th>INVOICE#</th>
        <th>CUST</th>
        <th>PAID</th>
        <th>SIDE</th>
        <th>O/F</th>
        <th>CRT#</th>
        <th>SEAL</th>
        <th>BALE</th>
        <th>WEIGHT</th>
        <th>BGK#</th>
        <th>ERD</th>
        <th>CUT OFF</th>
        <th>NOTE</th>
        <th>IN</th>
        <th>OUT</th>
        <th>BL</th>
        <th>PIC</th>
        <th>TRUCK</th>
        <th>PAID</th>
    </tr>

    <% @inventories.each do |inv| %>
        <tr>
            <td><%= link_to inv.invoice, inventory_path(inv) %></td>
            <td><%= inv.customer %></td>
            <td><%= inv.paid_date %></td>
            <td><%= inv.side %></td>
            <td><%= inv.o_f %></td>
            <td><%= inv.crt %></td>
            <td><%= inv.seal %></td>
            <td><%= inv.bale %></td>
            <td><%= inv.weight %></td>
            <td><%= inv.bgk %></td>
            <td><%= inv.erd %></td>
            <td><%= inv.cutoff_date %></td>
            <td><%= inv.note %></td>
            <td><%= inv.in_date %></td>
            <td><%= inv.out_date %></td>
            <td><%= inv.bl %></td>
            <td><%= inv.pic %></td>
            <td><%= inv.truck %></td>
            <td><%= inv.paid %></td>
        </tr>
    <% end %>
</table>
